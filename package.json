{
  "name": "alfred-femine",
  "version": "0.0.12",
  "description": "front end tools mine",
  "license": "MIT",
  "repository": "FeMiner/alfred-femine",
  "author": {
    "name": "FeMiner",
    "email": "muyuxingguang@qq.com",
    "url": "https://www.zxing.top"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "xo && ava",
    "fix": "xo --fix",
    "postinstall": "alfy-init",
    "preuninstall": "alfy-cleanup",
    "prepublishOnly": "node release.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fix && lint-staged && npm run test && npm run prepublishOnly"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "index.js",
    "icon.png",
    "info.plist"
  ],
  "keywords": [
    "alfred",
    "workflow",
    "femine"
  ],
  "dependencies": {
    "alfy": "*",
    "atob": "^2.1.2",
    "btoa": "^1.2.1",
    "hex-rgb": "^4.1.0",
    "rgb-hex": "^3.0.0"
  },
  "devDependencies": {
    "alfy-test": "*",
    "ava": "*",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "xo": "*"
  },
  "xo": {
    "prettier": true
  }
}
